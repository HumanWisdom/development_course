#/var/www/html/onboarding/.htaccess
RewriteEngine On

# Serve existing files and directories directly
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Prevent internal redirects for /onboarding/index.html
RewriteCond %{ENV:REDIRECT_STATUS} ^$
RewriteRule ^onboarding/index\.html - [L]

# Redirect all requests within the "onboarding" project to index.html
RewriteRule ^(.*)$ /onboarding/index.html [NC,L,QSA]